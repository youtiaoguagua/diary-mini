.body .bottom-item {
  position: fixed;
  width: 100%;
  height: 160rpx;
  bottom: 0;
  background-color: #FFFFFF;
  box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.1);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.body .bottom-item .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.body .bottom-item .item .iconfont {
  font-size: 36rpx;
}
.body .bottom-item .item text:last-child {
  font-size: 24rpx;
  font-weight: 600;
  margin: 10rpx 0;
}



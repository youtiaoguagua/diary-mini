.slot-wrap {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	padding-left: 20rpx;
	display: flex;
	font-size: 34rpx;
	color:  #909399;
}
.slot-wrap>view{
	margin-right: 42rpx;
}
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	height: 100vh;
}
.content-body {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	padding: 0 20rpx;
}
.title-input {
	height: 35px;
	border-bottom: 3rpx solid #000000;
	font-size: 41rpx;
	padding: 10rpx 0;
	box-sizing: border-box;
}
.html {
	width: 100%;
}
.editor {
	width: 100%;
}
.editor view {
	box-sizing: border-box;
}

